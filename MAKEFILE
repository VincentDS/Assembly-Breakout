debug: EMPTY.ASM KEYB.OBJ
	ML /Zi /W3 /WX KEYB.OBJ EMPTY.ASM /Fe EMPTY_D.EXE
release: EMPTY.ASM KEYB.OBJ
	ML /W3 /WX KEYB.OBJ EMPTY.ASM /Fe EMPTY.EXE
	
KEYB.OBJ: KEYB.ASM
	ML /c /W3 /WX /FoKEYB.OBJ /Ta KEYB.ASM
KEYBD.OBJ: KEYB.ASM
	ML /c /Zi /W3 /WX /FoKEYBD.OBJ /Ta KEYB.ASM

clean:
	del *.EXE
	del *.OBJ
	del *.BAK
cv:
	cv /s EMPTY_D.EXE
